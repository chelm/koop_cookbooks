{
    "server": {
        "port": 80
    },
    "processes":2,
    "logfile": "<%= node[:koop][:logfile] %>",
    "data_dir": "<%= node[:koop][:data_dir] %>",
    "s3": {
      "bucket": "<%= node[:koop][:s3][:bucket] %>"
    },

    "db": {
        "postgis": {
            "conn": "<%= node[:koop][:db][:postgis][:conn] %>"
        }
    },
    "agol":{
      "request_workers": {
        "redis":{
          "prefix":"<%= node[:koop][:agol][:request_workers][:prefix] %>",
          "port":<%= node[:koop][:agol][:request_workers][:port] %>,
          "host":"<%= node[:koop][:agol][:request_workers][:host] %>"
        }
      }
    }

    
}
